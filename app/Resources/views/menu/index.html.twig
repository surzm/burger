{% extends 'base.html.twig' %}

{% block body %}
    {{ include('header/index.html.twig') }}
    <div>
        {% for product in products %}
            <a href="/menu/{{ product.id }}"> <h3>{{ product.name }}</h3></a>
            <p> Цена :{{ product.price }}</p>
        {% else %}
            <p>No products found</p>
        {% endfor %}
    </div>
{% endblock %}